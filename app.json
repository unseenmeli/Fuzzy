{
  "expo": {
    "name": "LongVibe",
    "slug": "longvibe",
    "scheme": "longvibe",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unseenmeli.longvibe",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture profile photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select profile photos.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.unseenmeli.longvibe",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "/"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "/"
      },
      "eas": {
        "projectId": "f99218b5-aa94-47a8-bc4e-d420bd264cfb"
      }
    },
    "owner": "unseenmeli"
  }
}
